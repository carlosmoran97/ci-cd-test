{
    "env":{
        "browser": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jest/recommended"
    ],
    "parserOptions":{
        "ecmaFeatures":{
            "jsx": true
        },
        "ecmaVersion": 6,
        "sourceType":"module"
    },
    "plugins":["jest"],
    "rules":{
        "no-console":"off"
    },
    "settings": {
        "react":{
            "version":"16.0"
        }
    }
}
